<robot name="2DrobotArm">
    <link name="world"/>

    <joint name="worldTolink1" type="fixed">
        <parent link="world"/>
        <child link="link1"/>
        <origin xyz="0 0 0"/>
    </joint>

    <link name="link1">
        <visual>
            <origin rpy="0 1.57079632679 0" xyz="0 0 0.3"/>
            <geometry>
                <cylinder length="0.1" radius="0.01"/>
            </geometry>
        </visual>
    </link>

    <joint name="link1Tolink2" type="revolute">
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="0 0 0.3"/>
        <axis xyz="1 0 0"/>
        <dynamics damping="0.0" friction="0.0"/>
    </joint>

    <link name="link2">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <cylinder length="0.2" radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <cylinder length="0.2" radius="0.02"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.0 0.0 0.2"/>
            <mass value="1"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="link2Tolink3" type="revolute">
        <parent link="link2"/>
        <child link="link3"/>
        <origin xyz="0 0 0.3"/>
        <axis xyz="1 0 0"/>
    </joint>

    <link name="link3">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <cylinder length="0.2" radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <cylinder length="0.2" radius="0.02"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.0 0.0 0.2"/>
            <mass value="1"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>

    <joint name="link3Tolink4" type="prismatic">
        <parent link="link3"/>
        <child link="link4"/>
        <origin xyz="0 0 0.3"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name="link4">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <cylinder length="0.2" radius="0.02"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.2"/>
            <geometry>
                <cylinder length="0.2" radius="0.02"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.0 0.0 0.2"/>
            <mass value="1"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    
</robot>
